{
    "name": "smoothphp/cqrs-es-framework",
    "description": "Smooth Php CRQS Event Sourcing Framework",
    "keywords": [
        "smooth php",
        "Command Bus",
        "Event Sourcing",
        "CRQS"
    ],
    "homepage": "https://github.com/SmoothPhp/CQRS-ES-Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Bennett",
            "email": "simon@pixelatedcrow.im",
            "homepage": "https://pixelatedcrow.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0|8.*",
        "ramsey/uuid": "^4",
        "nesbot/carbon": "^2.0.0"
    },
    "replace": {
        "smoothphp/commandbus": "self.version",
        "smoothphp/contracts": "self.version",
        "smoothphp/domain": "self.version",
        "smoothphp/eventbus": "self.version",
        "smoothphp/eventdispatcher": "self.version",
        "smoothphp/eventsourcing": "self.version",
        "smoothphp/serialization": "self.version",
        "smoothphp/eventstore": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "SmoothPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmoothPhp\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
